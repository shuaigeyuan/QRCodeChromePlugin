{
    "manifest_version": 3,
    "name": "网页二维码",
    "version": "1.0",
    "description": "为当前网页生成优雅的二维码，支持在二维码中嵌入网站图标",
    "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
    },
    "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
    },
    "permissions": [
    "activeTab"
    ]
    }